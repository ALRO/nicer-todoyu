{
    "manifest_version": 2,

    "name": "Nicer Todoyu",
    "description": "Makes Todoyu nicer",
    "version": "0.1.0",

    "permissions": [],

    "web_accessible_resources": [
        "css/todoyu.css"
    ],

    "content_scripts": [{
        "js": [
            "lib/jquery-2.1.1.min.js",
            "js/todoyu.js"
        ],
        "matches": [
            "https://*.todoyu.net/*"
        ],
        "run_at": "document_end"
    }]
}